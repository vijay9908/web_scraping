from urllib.request import urlopen as uropenfrom bs4 import BeautifulSoup as BeautifulSoup
from bs4 import BeautifulSoup as BeautifulSoup
url=input("Enter url:")
req = urlopen(url)
htmltext= req.read()
req.close()
refine = BeautifulSoup(htmltext, "html.parser")
for i in refine.select(''):
	print(i.text)
